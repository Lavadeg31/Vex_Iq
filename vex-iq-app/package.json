      }
    }
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@supabase/supabase-js": "^2.39.3",
    "@types/base-64": "^1.0.2",
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20.11.19",
    "@types/pg": "^8.11.0",
    "axios": "^1.7.9",
    "base-64": "^1.0.0",
    "bcrypt": "^5.1.1",
    "date-fns": "^3.3.1",
    "dotenv": "^16.4.5",
    "expo": "~52.0.36",
    "expo-asset": "~11.0.4",
    "expo-av": "~15.0.2",
