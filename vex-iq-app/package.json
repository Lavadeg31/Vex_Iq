    "react-native": "0.76.7",
    "react-native-chart-kit": "^6.12.0",
    "react-native-dotenv": "^3.4.11",
    "react-native-markdown-display": "^7.0.2",
    "react-native-paper": "^5.12.3",
    "react-native-safe-area-context": "4.12.0",
